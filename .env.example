# Convex
CONVEX_DEPLOYMENT=
NEXT_PUBLIC_CONVEX_URL=

# OAuth
## GitHub OAuth
AUTH_GITHUB_ID=
AUTH_GITHUB_SECRET=
## Google OAuth
AUTH_GOOGLE_ID=
AUTH_GOOGLE_SECRET=

# Resend (Email)
RESEND_API_KEY=

# Web Push (VAPID Keys)
# 公開鍵：フロントエンドで使用（サブスクリプション登録時）
NEXT_PUBLIC_VAPID_PUBLIC_KEY=

# Note: VAPID秘密鍵はConvex環境変数に設定してください
# Convex Dashboard > Settings > Environment Variables
# VAPID_PRIVATE_KEY=your-private-key-here

# logger.ts
NEXT_PUBLIC_LOG_LEVEL=warn
